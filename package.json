{
  "name": "parse-server-relief-project",
  "version": "2.0.1",
  "description": "Relief service parse server",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dist": "sh ./scripts/dist.sh",
    "start": "node ./dist/index.js",
    "serve": "ts-node -r tsconfig-paths/register src/index.ts",
    "debug": "export CLOUD_CODE_MAIN=\"./src/cloud/main.ts\" && nodemon --watch \"src/**\" --watch \"src/**\" --ext \"ts,json\" --ignore \"src/**/*.spec.ts\" --exec \"npm run serve\"",
    "deploy": "npm run build && npm run dist && eb deploy"
  },
  "engines": {
    "node": ">=14.0",
    "ts-node": ">=10"
  },
  "dependencies": {
    "express": "4.18.1",
    "parse-server": "5.2.1",
    "mongodb": "^4.3.0",
    "parse": "3.4.1",
    "ts-node": "^10.2.1"
  },
  "devDependencies": {
    "@types/express": "4.17.13",
    "@types/node": "17.0.26",
    "@types/parse": "^2.18.14",
    "dotenv": "^16.0.1",
    "mongodb-runner": "4.9.0",
    "nodemon": "2.0.16",
    "ts-standard": "^11.0.0",
    "typescript": "^4.7.3"
  }
}
